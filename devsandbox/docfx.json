{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/swagger/*.json",
          "**/*.yml",
          "context/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "devsandbox/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/breadcrumb.json",
          "**/rest-products.json"
        ],
        "exclude": [
          "**/obj/**",
          "devsandbox/**",
          "**/includes/**"
        ]
      }
    ],
    "globalMetadata": {
      "searchScope": [
        "Team Foundation Server"
      ],
      "enable_rest_try_it": true,
      "signintest": true,
      "extendBreadcrumb": false,
      "breadcrumb_path": "/devsandbox/breadcrumb.json",
      "brand": "dynamics",
      "apiPlatform": "dotnet",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/SamplesPages",
      "feedback_product_url": "mailto://duncanma@microsoft.com",
      "force_uhf_ppe": true,
      "pdf_url_template": "https://docs.microsoft.com/pdfstore/en-us/VS.core-docs/{branchName}{pdfName}",
      "new-sign-in": true
    },
    "markdownEngineName": "markdig",
    "overwrite": [],
    "externalReference": [],
    "fileMetadata": {
      "featureFlags": {
        "conceptual/**/*.md": [
          "enable_uhf_ppe"
        ]
      },
      "searchScope": {
        "conceptual/**/*.md": [
          ".NET"
        ]
      }
    },
    "template": [],
    "dest": "devsandbox",
    "exportRawModel": true,
    "rawModelOutputFolder": "../_model",
    "exportViewModel": true,
    "viewModelOutputFolder": "../_viewmodel"
  }
}
